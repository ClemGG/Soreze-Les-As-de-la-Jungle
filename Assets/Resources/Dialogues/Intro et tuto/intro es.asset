  repliques:
  - text: "\xA1Hola, Salvador! \xBFQu\xE9 te trae por aqu\xED?"
    backgroundImg: {fileID: 21300000, guid: afb5d9f9ee373234482f5a0745ff4012, type: 3}
    character: {fileID: 11400000, guid: 5d3acadf3fdf0a940b12ba9fe6a95d17, type: 2}
