    character: {fileID: 11400000, guid: 5d3acadf3fdf0a940b12ba9fe6a95d17, type: 2}
    clip: {fileID: 0}
    mood: 0
    side: 1
    bubbleSize: 0
    arrowOrientation: 1
  - text: "Gracias amigos ! \n\xA1Para ayudarlo, perm\xEDtame mostrarle las magn\xEDficas
